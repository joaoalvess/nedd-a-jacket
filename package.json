    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@commitlint/config-conventional": "^13.1.0",
    "@react-native-community/eslint-config": "^2.0.0",
